{
  "name": "osc-min",
  "version": "1.1.3-pre",
  "main": "lib/index.min.js",
  "author": {
    "name": "Russell McClellan",
    "email": "russell.mcclellan@gmail.com",
    "url": "http://www.russellmcc.com"
  },
  "description": "Simple utilities for open sound control in node.js",
  "keywords": [
    "open sound control",
    "OSC",
    "music control",
    "NIME"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/russellmcc/node-osc-min.git"
  },
  "dependencies": {
    "binpack": "~0"
  },
  "devDependencies": {
    "mocha": "2.*",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "blanket": "*",
    "coffee-loader": "0.9.0",
    "coffeescript": "2.4.1",
    "coveralls": "*",
    "docket": "0.0.5",
    "jest": "^24.9.0",
    "jest-coffee-preprocessor": "^1.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "config": {
    "blanket": {
      "loader": "./node-loaders/coffee-script",
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "directories": {
    "lib": "lib",
    "examples": "examples"
  },
  "license": "Zlib",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "cake coverage",
    "coveralls": "cake coveralls",
    "doc": "cake doc",
    "build": "webpack-cli --mode production",
    "build:dev": "webpack-cli --progress --colors --watch --mode development"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.coffee$": "<rootDir>/node_modules/jest-coffee-preprocessor/index.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee"
    ],
    "modulePaths": [
      "src"
    ]
  },
  "_id": "osc-min@0.0.5",
  "_from": "osc-min@0.0.5",
  "dist": {
    "shasum": "ead2d1b1e7ff2382e4dad0f7aaa8aa20832118b1"
  }
}
